@model prjShoppingForum.Models.Entity.tUserProfile


    <div class="wrappers">
        <h3>會員基本資料</h3>
        <div class="form-row">
            <div class="form-holder">
                帳號: @Model.fUserId
            </div>
            <div class="form-holder">
                姓名: @Model.fName
            </div>
        </div>
        <div class="form-row">
            <div class="form-holder">
                性別: @Model.fGender
            </div>
            <div class="form-holder">
                生日: @Model.fBirthday
            </div>
        </div>
        <div class="form-row">
            <div class="form-holder">
                電話: @Model.fTel
            </div>
            <div class="form-holder">
                手機: @Model.fPhone
            </div>
        </div>
        <div class="form-row">
            <div class="form-holder">
                城市: @Model.fCity
            </div>
            <div class="form-holder">
                地址: @Model.fAddress
            </div>
        </div>
        
    </div>

<link rel="stylesheet" href="~/Content/styles.css">

@section Scripts{
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/mains.js"></script>
}



